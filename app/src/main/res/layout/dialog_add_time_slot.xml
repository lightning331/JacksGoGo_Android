<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="16dp"
    android:layout_marginRight="16dp"
    android:background="@color/JGGWhite"
    android:orientation="vertical">
    <LinearLayout
        android:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:orientation="vertical"
        android:gravity="center_vertical">
        <TextView
            android:text="Set start time:"
            android:fontFamily="@font/mulibold"
            android:textSize="@dimen/list_title_font_size"
            android:textColor="@color/JGGBlack"
            android:layout_marginTop="16dp"
            android:layout_marginLeft="16dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:orientation="horizontal"
            android:gravity="center">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/btn_add_start_time_up"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/button_showmore_green"/>
                <EditText
                    android:editable="false"
                    android:text="10"
                    android:id="@+id/txt_add_start_time"
                    android:layout_width="56dp"
                    android:layout_height="wrap_content"
                    android:inputType="none"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:textAlignment="center"
                    android:padding="@dimen/edit_text_padding_size"
                    android:fontFamily="@font/mulibold"
                    android:textSize="@dimen/list_title_font_size"
                    android:textColor="@color/JGGGrey2"
                    android:background="@drawable/grey_border_background"/>
                <ImageView
                    android:id="@+id/btn_add_start_time_down"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/button_showless_green"/>
            </LinearLayout>
            <TextView
                android:text=":"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAlignment="center"
                android:padding="@dimen/edit_text_padding_size"
                android:fontFamily="@font/mulibold"
                android:textSize="@dimen/body_font_size"
                android:textColor="@color/JGGBlack"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/btn_add_start_time_minute_up"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/button_showmore_green"/>
                <EditText
                    android:editable="false"
                    android:text="00"
                    android:id="@+id/txt_add_start_minute_time"
                    android:layout_width="56dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:textAlignment="center"
                    android:padding="@dimen/edit_text_padding_size"
                    android:fontFamily="@font/mulibold"
                    android:textSize="@dimen/list_title_font_size"
                    android:textColor="@color/JGGGrey2"
                    android:background="@drawable/grey_border_background"/>
                <ImageView
                    android:id="@+id/btn_add_start_time_minute_down"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/button_showless_green"/>
            </LinearLayout>
            <ImageView
                android:id="@+id/btn_add_start_time_am"
                android:text="AM"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:textSize="@dimen/list_title_font_size"/>
            <ImageView
                android:id="@+id/btn_add_start_time_pm"
                android:text="PM"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/JGGGrey4">
    </LinearLayout>
    <LinearLayout
        android:visibility="gone"
        android:id="@+id/advance_search_end_time_layout"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:gravity="center_vertical">
            <TextView
                android:text="Set end time:"
                android:fontFamily="@font/mulibold"
                android:textSize="@dimen/list_title_font_size"
                android:textColor="@color/JGGBlack"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">
            </LinearLayout>
            <ImageView
                android:id="@+id/btn_advance_search_close"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:src="@mipmap/button_close_round_green"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:orientation="horizontal"
            android:gravity="center">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/btn_add_end_time_up"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/button_showmore_green"/>
                <EditText
                    android:editable="false"
                    android:text="11"
                    android:id="@+id/txt_add_end_time"
                    android:layout_width="56dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:textAlignment="center"
                    android:padding="@dimen/edit_text_padding_size"
                    android:fontFamily="@font/mulibold"
                    android:textSize="@dimen/list_title_font_size"
                    android:textColor="@color/JGGGrey2"
                    android:background="@drawable/grey_border_background"/>
                <ImageView
                    android:id="@+id/btn_add_end_time_down"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/button_showless_green"/>
            </LinearLayout>
            <TextView
                android:text=":"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAlignment="center"
                android:padding="@dimen/edit_text_padding_size"
                android:fontFamily="@font/mulibold"
                android:textSize="@dimen/body_font_size"
                android:textColor="@color/JGGBlack"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/btn_add_end_time_minute_up"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/button_showmore_green"/>
                <EditText
                    android:editable="false"
                    android:text="00"
                    android:id="@+id/txt_add_end_minute_time"
                    android:layout_width="56dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:textAlignment="center"
                    android:padding="@dimen/edit_text_padding_size"
                    android:fontFamily="@font/mulibold"
                    android:textSize="@dimen/list_title_font_size"
                    android:textColor="@color/JGGGrey2"
                    android:background="@drawable/grey_border_background"/>
                <ImageView
                    android:id="@+id/btn_add_end_time_minute_down"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/button_showless_green"/>
            </LinearLayout>
            <ImageView
                android:id="@+id/btn_add_end_time_am"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"/>
            <ImageView
                android:id="@+id/btn_add_end_time_pm"
                android:text="PM"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/JGGGrey4">
    </LinearLayout>
    <LinearLayout
        android:visibility="visible"
        android:id="@+id/set_end_time_layout"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:gravity="center_vertical">
        <TextView
            android:id="@+id/btn_advance_search_end_time"
            android:text="Set End Time (Optional)"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginTop="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="16dp"
            android:padding="@dimen/edit_text_padding_size"
            android:textAlignment="center"
            android:fontFamily="@font/mulibold"
            android:textSize="@dimen/button_font_size"
            android:textColor="@color/JGGGreen"
            android:background="@drawable/green_border_background">
        </TextView>
    </LinearLayout>
    <LinearLayout
        android:visibility="gone"
        android:id="@+id/add_time_pax_background"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="26dp"
        android:gravity="center_vertical">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="40"
            android:gravity="center_vertical">
            <TextView
                android:text="No. Of Pax:"
                android:fontFamily="@font/mulibold"
                android:textSize="@dimen/list_title_font_size"
                android:textColor="@color/JGGBlack"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="60"
            android:gravity="center_vertical">
            <ImageView
                android:id="@+id/btn_add_time_pax_left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/button_previous_green"/>
            <EditText
                android:editable="false"
                android:text="2"
                android:id="@+id/txt_add_time_pax"
                android:layout_width="56dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:textAlignment="center"
                android:padding="@dimen/edit_text_padding_size"
                android:fontFamily="@font/mulibold"
                android:textSize="@dimen/body_font_size"
                android:textColor="@color/JGGGrey2"
                android:background="@drawable/grey_border_background"/>
            <ImageView
                android:id="@+id/btn_add_time_pax_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/button_right_green"/>

        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/JGGGrey4">
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@id/lbl_alert_description">

        <TextView
            android:id="@+id/btn_add_time_cancel"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@color/JGGGreen10Percent"
            android:textColor="@color/JGGGreen"
            android:fontFamily="@font/mulibold"
            android:textSize="@dimen/list_title_font_size"
            android:text="Cancel"/>

        <TextView
            android:id="@+id/btn_add_time_ok"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@color/JGGGreen"
            android:textColor="@color/JGGWhite"
            android:fontFamily="@font/mulibold"
            android:textSize="@dimen/list_title_font_size"
            android:text="Done"/>
    </LinearLayout>

</LinearLayout>
