<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="240dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.synnapps.carouselview.CarouselView
        android:background="@mipmap/appointment_placeholder"
        android:id="@+id/detail_images_carousel_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:fillColor="@color/JGGGrey4"
        app:pageColor="#00000000"
        app:radius="5dp"
        app:slideInterval="6000"
        app:pageTransformInterval="1000"
        app:strokeColor="@color/JGGGrey2"
        app:strokeWidth="1dp" />
</LinearLayout>
