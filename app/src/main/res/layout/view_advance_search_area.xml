<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="16dp"
    android:layout_marginRight="16dp"
    android:background="@color/JGGWhite"
    android:orientation="vertical">
    <ImageButton
        android:id="@+id/btn_area_close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:background="@color/JGGGrey5"
        android:src="@mipmap/button_area_tick_round_green"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:gravity="center_vertical">
        <ImageView
            android:id="@+id/btn_area_cbd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/checkbox_on_green"/>
        <TextView
            android:text="CBD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:textColor="@color/JGGBlack"
            android:background="@color/transparent"
            android:fontFamily="@font/muliregular"
            android:textSize="@dimen/list_title_font_size" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:gravity="center_vertical">
        <ImageView
            android:id="@+id/btn_area_central"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/checkbox_on_green"/>
        <TextView
            android:text="Central South"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:textColor="@color/JGGBlack"
            android:background="@color/transparent"
            android:fontFamily="@font/muliregular"
            android:textSize="@dimen/list_title_font_size" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:gravity="center_vertical">
        <ImageView
            android:id="@+id/btn_area_eunos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/checkbox_on_green"/>
        <TextView
            android:text="Eunos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:textColor="@color/JGGBlack"
            android:background="@color/transparent"
            android:fontFamily="@font/muliregular"
            android:textSize="@dimen/list_title_font_size" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:gravity="center_vertical">
        <ImageView
            android:id="@+id/btn_area_orchard"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/checkbox_on_green"/>
        <TextView
            android:text="Orchard"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:textColor="@color/JGGBlack"
            android:background="@color/transparent"
            android:fontFamily="@font/muliregular"
            android:textSize="@dimen/list_title_font_size" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:gravity="center_vertical">
        <ImageView
            android:id="@+id/btn_area_newton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/checkbox_on_green"/>
        <TextView
            android:text="Newton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:textColor="@color/JGGBlack"
            android:background="@color/transparent"
            android:fontFamily="@font/muliregular"
            android:textSize="@dimen/list_title_font_size" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:gravity="center_vertical">
        <ImageView
            android:id="@+id/btn_area_toa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/checkbox_on_green"/>
        <TextView
            android:text="Toa Payoh"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:textColor="@color/JGGBlack"
            android:background="@color/transparent"
            android:fontFamily="@font/muliregular"
            android:textSize="@dimen/list_title_font_size" />
    </LinearLayout>

</LinearLayout>
